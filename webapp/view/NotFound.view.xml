<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	controllerName="gantt.chart.app.controller.NotFound">

	<IllustratedMessage 
		illustrationType="sapIllus-PageNotFound"
		enableVerticalResponsiveness="true">
		<additionalContent>
			<Button
				press="onNavToProductsOverview" 
				text="{i18n>btnGoToProductsOverview}"
				type="Emphasized" />
		</additionalContent>
	</IllustratedMessage>

</mvc:View>